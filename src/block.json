{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "jc-blocks/jc-simple-blurb",
	"version": "0.1.0",
	"title": "Simple Blurb",
	"category": "text",
	"description": "A block that adds a simple blurb (short piece of text and image).",
	"keywords": [ "image", "text", "blurb" ],
	"textdomain": "jc-simple-blurb",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h4"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p"
		}
	},
	"supports": {
		"html": false
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}